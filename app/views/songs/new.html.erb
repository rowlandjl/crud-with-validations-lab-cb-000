<h1>New Song Form</h1>

<%= form_for @song do |f| %>
  <%= f.text_field :title, @song.title %>
  <%= f.text_field :release_year, @song.release_year %>
  <%= f.text_field :released, @song.released %>
  <%= f.text_field :genre, @song.genre %>
  <%= f.text_field :artist_name, @song.artist_name %>

  <%= f.submit %>
<% end %>
